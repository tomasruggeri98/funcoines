Console.WriteLine("ingresar numero para conseguir raiz cuadrada:");
double numero = double.Parse(Console.ReadLine());

double raizCuadrada = CalcularRaizCuadrada(numero);

Console.WriteLine("La ra√≠z cuadrada de " + numero + " es: " + raizCuadrada);


double CalcularRaizCuadrada(double numero)
{
    return Math.Sqrt(numero);
}
